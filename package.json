{
  "name": "evalkit",
  "version": "0.2.0",
  "description": "Lightweight deterministic evaluators for AI agents. Binary pass/fail checks, zero dependencies, no LLM cost.",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "prepublishOnly": "npm run build && npm test"
  },
  "keywords": [
    "ai",
    "agent",
    "evals",
    "evaluation",
    "testing",
    "golden-set",
    "deterministic",
    "langchain",
    "llm",
    "evalkit"
  ],
  "type": "module",
  "license": "MIT",
  "author": "Walid Khori",
  "repository": {
    "type": "git",
    "url": "https://github.com/wkhori/evalkit.git"
  },
  "homepage": "https://github.com/wkhori/evalkit#readme",
  "bugs": {
    "url": "https://github.com/wkhori/evalkit/issues"
  },
  "devDependencies": {
    "@types/node": "^25.3.2",
    "tsup": "^8.5.1",
    "typescript": "^5.4.0",
    "vitest": "^4.0.18"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ]
}
